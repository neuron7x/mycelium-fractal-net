{
  "name": "medium",
  "description": "Medium configuration for standard experiments",
  "validation": {
    "seed": 42,
    "epochs": 5,
    "batch_size": 16,
    "grid_size": 64,
    "steps": 64,
    "device": "cpu"
  },
  "model": {
    "input_dim": 4,
    "hidden_dim": 32,
    "use_sparse_attention": true,
    "use_stdp": true
  },
  "simulation": {
    "turing_enabled": true,
    "turing_threshold": 0.75,
    "quantum_jitter": true,
    "jitter_var": 0.0005
  },
  "federated": {
    "num_clusters": 50,
    "byzantine_fraction": 0.2,
    "sample_fraction": 0.1
  },
  "expected_metrics": {
    "runtime_seconds_max": 30,
    "memory_mb_max": 200,
    "fractal_dim_range": [1.4, 1.9],
    "lyapunov_max": 0
  }
}
