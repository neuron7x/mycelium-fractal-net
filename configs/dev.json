{
  "name": "dev",
  "description": "Development environment configuration with verbose logging and relaxed limits",
  "environment": "development",
  "api": {
    "host": "0.0.0.0",
    "port": 8000,
    "reload": true,
    "debug": true,
    "cors_origins": ["*"],
    "docs_enabled": true
  },
  "logging": {
    "level": "DEBUG",
    "format": "text",
    "include_request_body": true
  },
  "validation": {
    "seed": 42,
    "epochs": 1,
    "batch_size": 4,
    "grid_size": 32,
    "steps": 32,
    "device": "cpu"
  },
  "model": {
    "input_dim": 4,
    "hidden_dim": 16,
    "use_sparse_attention": true,
    "use_stdp": true
  },
  "simulation": {
    "turing_enabled": true,
    "turing_threshold": 0.75,
    "quantum_jitter": false,
    "jitter_var": 0.0005
  },
  "federated": {
    "num_clusters": 10,
    "byzantine_fraction": 0.2,
    "sample_fraction": 0.1
  },
  "limits": {
    "max_grid_size": 256,
    "max_steps": 1000,
    "max_samples": 10000,
    "request_timeout_seconds": 300
  },
  "expected_metrics": {
    "runtime_seconds_max": 60,
    "memory_mb_max": 500,
    "fractal_dim_range": [0.1, 2.5],
    "lyapunov_max": 1.0
  }
}
